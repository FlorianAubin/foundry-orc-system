<div class="active">

<table class="item-list status">
    <tr class="main-header flexrow">
      <th class="content">{{localize "orc.character.combatStyle"}}</th>
    </tr>
    <tr class="header flexrow">
      <th class="content"></th>
      <th class="content">{{localize "orc.character.attack"}}</th>
      <th class="content">{{localize "orc.character.defence"}}</th>
      <th class="content">{{localize "orc.character.dodge"}}</th>
      <th class="content">{{localize "orc.character.damageBonus"}}</th>
    </tr>
    <tr class="item flexrow">
      <td class="content"></td>
      <td class="content">{{actor.system.combatStyle.attack}}</td>
      <td class="content">{{actor.system.combatStyle.defence}}</td>
      {{#if actor.system.dodge.enable}}
        <td class="content">{{actor.system.combatStyle.dodge}}</td>
      {{else}}
        <td class="content fas fa-minus"></td>
      {{/if}}
      <td class="content">{{actor.system.combatStyle.damageBonus}}</td>
    </tr>

  <tr class="main-header flexrow">
      <th class="content">{{localize "orc.weapons"}}</th>
    </tr>
    <tr class="header flexrow">
      <th class="content"></th>
      <th class="content">{{localize "orc.weapon.defenceModif"}}</th>
    </tr>
    {{#each weapons as |weapon id|}}
    {{#if (and weapon.system.equipped weapon.system.defenceModif)}}
    <tr class="item flexrow">
      <td class="content">{{weapon.name}}</td>
      <td class="content">{{weapon.system.defenceModif}}</td>
    </tr>
    {{/if}}
    {{/each}}
    <tr class="main-header flexrow">
      <th class="content">{{localize "orc.equipableitems"}}</th>
    </tr>
    <tr class="header flexrow">
      <th class="content"></th>
      <th class="content">{{localize "orc.equipableitem.nSpell"}}</th>
      <th class="content">{{localize "orc.equipableitem.nInvoc"}}</th>
      <th class="content">{{localize "orc.equipableitem.magicPower"}}</th>
      <th class="content">{{localize "orc.equipableitem.mpReduc"}}</th>
      <th class="content">{{localize "orc.equipableitem.rollSpellBonus"}}</th>
    </tr>
    {{#each equipableitems as |equipableitem id|}}
    {{#if (and equipableitem.system.equipped
              (or equipableitem.system.nSpell 
                  equipableitem.system.nInvoc 
                  (isNotEmptyString equipableitem.system.magicPower)
                  equipableitem.system.mpReduc 
                  equipableitem.system.rollSpellBonus))}}
    <tr class="item flexrow">
      <td class="content">{{equipableitem.name}}</td>
      {{#if equipableitem.system.nSpell}}
        <td class="content">{{equipableitem.system.nSpell}}</td>
      {{/if}}
      {{#if equipableitem.system.nInvoc}}
        <td class="content">{{equipableitem.system.nInvoc}}</td>
      {{/if}}
      {{#if (isNotEmptyString equipableitem.system.magicPower)}}
        <td class="content">{{equipableitem.system.magicPower}}</td>
      {{/if}}
      {{#if equipableitem.system.mpReduc}}
        <td class="content">{{equipableitem.system.mpReduc}}</td>
      {{/if}}
      {{#if equipableitem.system.rollSpellBonus}}
        <td class="content">{{equipableitem.system.rollSpellBonus}}</td>
      {{/if}}
    </tr>
    {{/if}}
    {{/each}}
  </table>

